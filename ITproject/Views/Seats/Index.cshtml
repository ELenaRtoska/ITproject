@model IEnumerable<ITproject.Models.Seat>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SeatStatus)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.buildingId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SeatNumber)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.SeatStatus)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.buildingId)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.SeatNumber)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.SeatNumber }) |
        @Html.ActionLink("Details", "Details", new { id = item.SeatNumber }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.SeatNumber })
    </td>
</tr>
}

</table>
